<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageProjectUrl>https://github.com/Robson-Rocha/Xutils.DynamicCode</PackageProjectUrl>
    <RepositoryUrl>https://github.com/Robson-Rocha/Xutils.DynamicCode</RepositoryUrl>
    <Authors>Robson Rocha de Araújo</Authors>
    <Copyright>Copyright ©2019 Robson Rocha de Araújo</Copyright>
    <Description>Small library for dynamic C# code compilation</Description>
    <PackageIconUrl>https://github.com/Robson-Rocha/Xutils.DynamicCode/raw/master/docs/dynamiccode-nuget.png</PackageIconUrl>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.CodeAnalysis.CSharp" Version="3.2.1" />
    <PackageReference Include="Microsoft.CSharp" Version="4.5.0" />
    <PackageReference Include="Newtonsoft.Json" Version="13.0.2" />
    <PackageReference Include="System.Runtime.Loader" Version="4.3.0" />
  </ItemGroup>

</Project>
